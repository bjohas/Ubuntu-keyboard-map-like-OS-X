
keycode  49 = grave asciitilde grave notsign bar bar
keycode  94 = grave asciitilde backslash bar bar brokenbar bar brokenbar

clear control
clear lock
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

! Keyboard key labels
! 66/CAPSLOCK
! 50/Shift - 49/"\|"
! 37/Ctrl - Fn - 133/Win - 64/Alt - space - 108/Alt - 105/ctrl

keycode  66 = Control_L Control_L  Control_L  Control_L
keycode  37 = Super_L Super_L Super_L Super_L
keycode 133 = Alt_L Alt_L Alt_L Alt_L
keycode  64 = Hyper_L Hyper_L Hyper_L Hyper_L
keycode 108 = Hyper_R Hyper_R Hyper_R Hyper_R
keycode 105 = Alt_R Alt_R Alt_R Alt_R

add control = Control_L Control_R
add mod2 = Num_Lock
add mod1 = Alt_R Alt_L
add mod3 = Hyper_L Hyper_R
add mod4 = Super_L Super_R
